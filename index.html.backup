            width: 300px;
            height: 300px;
            background: rgba(255, 159, 10, 0.5);
            top: 10%;
            left: 15%;
            animation-delay: 0s;
        }

        .light-spot:nth-child(2) {
            width: 250px;
            height: 250px;
            background: rgba(191, 90, 242, 0.5);
            bottom: 20%;
            right: 10%;
            animation-delay: 5s;
        }

        .light-spot:nth-child(3) {
            width: 200px;
            height: 200px;
            background: rgba(64, 200, 224, 0.5);
            bottom: 15%;
            left: 25%;
            animation-delay: 10s;
        }

        .light-spot:nth-child(4) {
            width: 180px;
            height: 180px;
            background: rgba(255, 55, 95, 0.5);
            top: 30%;
            right: 20%;
            animation-delay: 15s;
        }

        @keyframes float-light {
            0% {
                transform: translate(0, 0) scale(1);
                opacity: 0.08;
            }
            50% {
                transform: translate(30px, -20px) scale(1.4);
                opacity: 0.12;
            }
            100% {
                transform: translate(-20px, 40px) scale(1);
                opacity: 0.08;
            }
        }

        /* 添加页面滚动动画 */
        .container {
            animation: fade-in 1s ease-out;
        }

        @keyframes fade-in {
            0% {
                transform: translateY(30px);
                opacity: 0;
            }
            100% {
                transform: translateY(0);
                opacity: 1;
            }
        }

        /* 给header添加更多视觉元素 */
        header {
            position: relative;
            overflow: visible;
        }

        /* 添加3D视觉效果 */
        h1 {
            transform: perspective(800px) rotateX(5deg);
            text-shadow: 0 4px 12px rgba(0, 0, 0, 0.4);
            transition: transform 0.5s ease;
        }

        h1:hover {
            transform: perspective(800px) rotateX(10deg) scale(1.02);
        }

        /* 添加3D悬浮动画效果 */
        .level-card {
            transition: transform 0.5s cubic-bezier(0.2, 0.8, 0.2, 1), box-shadow 0.5s ease;
            position: relative;
            transform-style: preserve-3d;
            overflow: visible;
        }

        .level-card:hover {
            transform: translateY(-10px) scale(1.03) rotateX(5deg);
            box-shadow: 0 20px 40px rgba(0,0,0,0.2);
            z-index: 2;
        }

        /* 添加炫酷的标题样式 */
        .level-title {
            background: linear-gradient(135deg, #FF9F0A, #FF375F);
            -webkit-background-clip: text;
            background-clip: text;
            -webkit-text-fill-color: transparent;
            position: relative;
            display: inline-block;
            font-weight: 800;
            letter-spacing: -0.03em;
            font-size: 1.5rem;
            margin-bottom: 15px;
        }

        /* 添加标题发光效果 */
        .level-title::after {
            content: '';
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            filter: blur(12px);
            opacity: 0.6;
            background: linear-gradient(135deg, rgba(255, 159, 10, 0.3), rgba(255, 55, 95, 0.3));
            transform: translateY(4px);
            z-index: -1;
            transition: opacity 0.3s ease, filter 0.3s ease;
        }

        .level-card:hover .level-title::after {
            opacity: 0.8;
            filter: blur(8px);
        }

        /* 添加页面内容流动动画 */
        @keyframes flow-in {
            0% {
                opacity: 0;
                transform: translateY(30px);
            }
            100% {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .levels-grid > * {
            animation: flow-in 0.6s ease-out forwards;
            opacity: 0;
        }

        /* 为每个卡片添加递增的延迟 */
        .level-card:nth-child(1) { animation-delay: 0.1s; }
        .level-card:nth-child(2) { animation-delay: 0.2s; }
        .level-card:nth-child(3) { animation-delay: 0.3s; }
        .level-card:nth-child(4) { animation-delay: 0.4s; }
        .level-card:nth-child(5) { animation-delay: 0.5s; }
        .level-card:nth-child(6) { animation-delay: 0.6s; }
        .level-card:nth-child(7) { animation-delay: 0.7s; }
        .level-card:nth-child(8) { animation-delay: 0.8s; }
        .level-card:nth-child(9) { animation-delay: 0.9s; }
        .level-card:nth-child(10) { animation-delay: 1.0s; }

        /* 为完成状态添加渐变背景 */
        .completed-status {
            background: linear-gradient(135deg, rgba(50, 215, 75, 0.8), rgba(50, 215, 75, 0.4));
            text-shadow: 0 1px 3px rgba(0,0,0,0.3);
            font-weight: 700;
            letter-spacing: 0.05em;
        }
    </style>
</head>
<body>
    <div class="container">
        <!-- 头部和标题区 -->
        <header>
            <h1>神奇喵猫<span class="title-accent">Python</span>训练营</h1>
        </header>
        
        <!-- 头部操作按钮区 -->
        <div class="header-actions">
            <a href="mistakes.html" class="mistake-btn">查看错题集</a>
            <a href="editor.html" class="editor-btn">题库编辑器</a>
        </div>
        
        <!-- 背景光效 -->
        <div class="background-effects">
            <div class="light-spot"></div>
            <div class="light-spot"></div>
            <div class="light-spot"></div>
            <div class="light-spot"></div>
        </div>
        
        <!-- 直接展示关卡网格 -->
        <div class="levels-grid">
            <!-- ... existing code ... -->
        </div>
    </div>
    <!-- ... existing code ... -->
</body>
